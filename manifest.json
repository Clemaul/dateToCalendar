{

  "manifest_version": 2,
  "name": "dateToCalendar",
  "version": "1.0",

  "description": "read every dates in webpage and add button to sync to google calendar",

  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.3.1.min.js","chrono-master/chrono.js","dateToCalendar.js"],
      "css": ["style.css"]
    }
  ]

}